{
  "name": "Google Drive Realtime",
  "version": "1",
  "manifest_version": 2,
  "description": "Google Drive Realtime Quickstart",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
	"experimental", 
	{"socket":["tcp-connect", "tcp-listen"]},
	"webview",
	"storage",
	"clipboardRead",
	"clipboardWrite"
  ]
}
